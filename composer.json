    "elipettingale/case-converter": "^1.4",
    "elipettingale/laravel-entity-transformer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "EliPett\\ApiQueryLanguage\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Elliot Pettingale",
      "email": "elipettingale@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
